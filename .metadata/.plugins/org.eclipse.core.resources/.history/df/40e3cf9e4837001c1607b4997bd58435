 package amstrongrange;
public class amstrongrange 
{

	public static void main(String[] args) 
	{
		System.out.println("amstrong numbers form 100 to 999"); 
		int i=100;
		while (i<1000)
		{
				int sum=0;
				int num= i;
				while(num>0)
			{
				int a=num%10;
				sum =sum + (a*a*a);
				num=num/10;
				return ;
			}		
				i++;
				
				if(sum == i)
				System.out.println(i);
		}	
		
		
	}

}
