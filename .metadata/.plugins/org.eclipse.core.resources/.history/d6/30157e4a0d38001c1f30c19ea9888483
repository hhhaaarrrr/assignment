
public class singleton {

	public static void main(String[] args) {
	
		star obj1 = star.getInstance();
		star obj2 = star.getInstance();
	}

}
class star
{
	static star obj = new star();
	private star()
	{
		System.out.println("star is shining");
	}
	public static star getInstance()
	{
		return obj;
	}

}

