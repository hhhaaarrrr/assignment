
public class stringclass {

	public static void main(String[] args) {
		String str ="Hello World";
		int len = str.length();
		
		System.out.println("length of given string is :" + " "+ len);

	
		String str1 = " Hello";
		str1 = str1.concat("  How  are you");
		System.out.println("new string is :"+ " "+ str1);
		
		
	
	
	}

}

